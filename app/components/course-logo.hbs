{{#if @course.isDocker}}
  <img alt={{@course.name}} src="/assets/images/challenge-logos/challenge-logo-docker.svg" ...attributes />
{{else if @course.isGit}}
  <img alt={{@course.name}} src="/assets/images/challenge-logos/challenge-logo-git.svg" ...attributes />
{{else if @course.isReact}}
  <img alt={{@course.name}} src="/assets/images/challenge-logos/challenge-logo-react.svg" ...attributes />
{{else if @course.isRedis}}
  <img alt={{@course.name}} src="/assets/images/challenge-logos/challenge-logo-redis.svg" ...attributes />
{{else if @course.isSQLite}}
  <img alt={{@course.name}} src="/assets/images/challenge-logos/challenge-logo-sqlite.svg" ...attributes />
{{/if}}